{
  "name": "uber-starter",
  "version": "0.0.1",
  "description": "Starter code for Uber projects.",
  "keywords": [
    "Uber",
    "React.js",
    "Webpack",
    "Express.js",
    "PostgreSQL",
    "Sequelize"
  ],
  "main": "src/server/index.mjs",
  "scripts": {
    "postinstall": "touch secrets.js",
    "seed": "NODE_ENV=development SEED_NUM=150 node --experimental-modules scripts/seed.js && echo \"\u001b[92;1mFinished seeding database.\u001b[0m\n\" && exit 0",
    "dev": "NODE_ENV=development HMR=disabled nodemon --experimental-modules --trace-deprecation src/server/index.mjs",
    "hot": "NODE_ENV=development HMR=enabled node --experimental-modules src/server/index.mjs",
    "prod": "NODE_ENV=production npx webpack --config=configs/webpack.prod.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuehlein/uber-starter.git"
  },
  "author": "Kyle Uehlein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kuehlein/uber-starter/issues"
  },
  "homepage": "https://github.com/kuehlein/uber-starter#readme",
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@loadable/component": "^5.5.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-pg-simple": "^5.0.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-cookie": "^3.0.8",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "sequelize": "^4.42.0",
    "webpack": "^4.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
