{
  "name": "uber-starter",
  "version": "0.0.1",
  "description": "Starter code for Uber projects.",
  "keywords": [
    "Uber",
    "schedule",
    "calendar",
    "react",
    "express",
    "postgresql"
  ],
  "main": "src/server/index.js",
  "scripts": {
    "postinstall": "touch secrets.js",
    "seed": "NODE_ENV=development SEED_NUM=150 node scripts/seed.js && echo \"\u001b[92;1mFinished seeding database.\u001b[0m\n\" && exit 0",
    "dev": "NODE_ENV=development HMR=disabled nodemon sourcemap/server/index.js",
    "hot": "NODE_ENV=development HMR=enabled node src/server/index.js",
    "prod": "NODE_ENV=production npx webpack --config=configs/webpack.prod.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuehlein/uber-starter.git"
  },
  "author": "Kyle Uehlein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kuehlein/uber-starter/issues"
  },
  "homepage": "https://github.com/kuehlein/uber-starter#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-pg-simple": "^5.0.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-cookie": "^3.0.8",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "sequelize": "^4.42.0",
    "webpack": "^4.29.0"
  },
  "devDependencies": {
    "css-loader": "^2.1.0",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "react-hot-loader": "^4.6.5",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-hot-middleware": "^2.24.3"
  }
